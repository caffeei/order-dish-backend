{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "libs/db/(.*)": "<rootDir>/../libs/@libs/db/src/$1",
    "libs/db": "<rootDir>/../libs/@libs/db/src",
    "@app/db-module/(.*)": "<rootDir>/../libs/db-module/src/$1",
    "@app/db-module": "<rootDir>/../libs/db-module/src",
    "@app/db/(.*)": "<rootDir>/../libs/db/src/$1",
    "@app/db": "<rootDir>/../libs/db/src"
  }
}